{
  "name": "@notimeco/tiny-ts-result",
  "version": "0.1.0",
  "repository": "https://github.com/notimeco/tiny-ts-result",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=24.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "!dist/examples/*",
    "!dist/**/*.spec.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "fmt": "prettier --log-level warn --cache --check .",
    "fmt:fix": "prettier --log-level warn --cache --write .",
    "lint": "eslint --cache --max-warnings 0 src",
    "lint:fix": "eslint --cache --max-warnings 0 --fix src",
    "test": "vitest --run --coverage --silent"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@eslint/markdown": "^7.4.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {}
}
